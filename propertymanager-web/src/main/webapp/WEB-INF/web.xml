<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Application</display-name>
	<servlet>
		<servlet-name>home</servlet-name>
		<jsp-file>/home.html</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>homedev</servlet-name>
		<jsp-file>/homedev.html</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>homelocal</servlet-name>
		<jsp-file>/homelocal.html</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>home</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>homedev</servlet-name>
		<url-pattern>/homedev</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>homelocal</servlet-name>
		<url-pattern>/homelocal</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>/homelocal.html</welcome-file>
	</welcome-file-list>
</web-app>
